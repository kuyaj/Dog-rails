<h3>Create a Dog</h3>
<%= form_with model: @dog do |form| %>
  <%= form.text_field :name, placeholder: "name" %>
  <%= form.text_field :motto, placeholder: "motto" %>

  <%= form.submit "Create dog", class: "button-default" %>
<% end %>

<br><br>
<%= link_to "Back", root_path, class: "button-control"%>